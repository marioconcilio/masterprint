<% content_for :navbar do %>
  <%= render 'estoque/navbar' %>
<% end %>

<% provide(:url, estoque_variados_path) %>
<% provide(:new_url, new_estoque_variado_path) %>
<% provide(:search_field, :nome_cont) %>
<% provide(:placeholder, 'Nome') %>

<div class="container-fluid">

  <!-- header -->
  <div class="row">
    <div class="col-lg-12">
      <div class="page-header">
        <h1>Variados</h1>
      </div>
    </div>
  </div>

  <!-- search form -->
  <div class="row">
    <div class="col-lg-12">
      <%= render 'shared/search_form' %>
    </div>
  </div>

  <!-- table view -->
  <div class="row row-margin">
    <div class="col-lg-12">
      <div id="var-tableview"><%= render 'table' %></div>
    </div>
  </div>

</div>
